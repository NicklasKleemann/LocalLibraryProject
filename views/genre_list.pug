extends layout

block content
  header 
    input(type="text" id="searchinput" placeholder="Search in genres...")
    button.btn(type="button" id="searchbutton") Go to genre -&gt
  h1= title

  ul.sidebar-nav(id="List")
    each val in list_genres
      li
        a(href=val.url) #{val.name}

    else
      li There are no genre.
  script(src="/javascripts/searchscript.js")